{
  "patientSearch": {
    "customAttributes": [
      {
        "translationKey": "REGISTRATION_PATIENT_SEARCH_DROPDOWN_PHONE_NUMBER",
        "fields": [
          "phoneNumber",
          "alternatePhoneNumber"
        ],
        "expectedFields": [
          {
            "field": "phoneNumber",
            "translationKey": "REGISTRATION_PATIENT_SEARCH_HEADER_PHONE_NUMBER"
          },
          {
            "field": "alternatePhoneNumber",
            "translationKey": "REGISTRATION_PATIENT_SEARCH_HEADER_ALTERNATE_PHONE_NUMBER"
          }
        ],
        "type": "person"
      },
      {
        "translationKey": "REGISTRATION_PATIENT_SEARCH_DROPDOWN_EMAIL",
        "fields": [
          "email"
        ],
        "expectedFields": [
          {
            "field": "email",
            "translationKey": "REGISTRATION_PATIENT_SEARCH_HEADER_EMAIL"
          }
        ],
        "type": "person"
      },
      {
        "translationKey": "REGISTRATION_PATIENT_SEARCH_DROPDOWN_VILLAGE",
        "fields": [
          "city_village"
        ],
        "expectedFields": [
          {
            "field": "city_village",
            "translationKey": "REGISTRATION_PATIENT_SEARCH_HEADER_VILLAGE"
          }
        ],
        "type": "address"
      },
      {
        "translationKey": "REGISTRATION_PATIENT_SEARCH_DROPDOWN_LOCALITY",
        "fields": [
          "address2"
        ],
        "expectedFields": [
          {
            "field": "address2",
            "translationKey": "REGISTRATION_PATIENT_SEARCH_HEADER_LOCALITY"
          }
        ],
        "type": "address"
      }
    ]
  },
  "defaultVisitType": "OPD",
  "patientInformation": {
    "defaultIdentifierPrefix": "ABC",
    "patientNameDisplayOrder": [
      "firstName",
      "middleName",
      "lastName"
    ],
    "showBirthTime": true,
    "showDOBEstimated": true,
    "showEnterManually": false,
    "showMiddleName": true,
    "showLastName": true,
    "isFirstNameMandatory": true,
    "isMiddleNameMandatory": false,
    "isGenderMandatory": true,
    "isDateOfBirthMandatory": true,
    "isLastNameMandatory": true,
    "addressHierarchy": {
      "showAddressFieldsTopDown": false,
      "strictAutocompleteFromLevel": "postalCode",
      "expectedFields": [
        {
          "addressField": "address1",
          "translationKey": "CREATE_PATIENT_HOUSE_NUMBER"
        },
        {
          "addressField": "address2",
          "translationKey": "CREATE_PATIENT_LOCALITY"
        },
        {
          "addressField": "cityVillage",
          "translationKey": "CREATE_PATIENT_CITY"
        },
        {
          "addressField": "countyDistrict",
          "translationKey": "CREATE_PATIENT_DISTRICT"
        },
        {
          "addressField": "stateProvince",
          "translationKey": "CREATE_PATIENT_STATE"
        },
        {
          "addressField": "postalCode",
          "translationKey": "CREATE_PATIENT_PINCODE"
        }
      ]
    },
    "additionalPatientInformation": {
      "translationKey": "REGISTRATION_ADDITIONAL_PATIENT_INFORMATION_TITLE",
      "attributes": [
        {
          "field": "distanceFromCenter",
          "translationKey": "REGISTRATION_DISTANCE_FROM_CENTER"
        },
        {
          "field": "isUrban",
          "translationKey": "REGISTRATION_IS_URBAN"
        },
        {
          "field": "cluster",
          "translationKey": "REGISTRATION_CLUSTER"
        },
        {
          "field": "RationCard",
          "translationKey": "REGISTRATION_RATION_CARD"
        },
        {
          "field": "familyIncome",
          "translationKey": "REGISTRATION_FAMILY_INCOME"
        },
        {
          "field": "debt",
          "translationKey": "REGISTRATION_DEBT"
        },
        {
          "field": "email",
          "translationKey": "REGISTRATION_EMAIL"
        }
      ]
    },
    "contactInformation": {
      "translationKey": "REGISTRATION_CONTACT_INFORMATION_TITLE",
      "attributes": [
        {
          "field": "phoneNumber",
          "translationKey": "REGISTRATION_PHONE_NUMBER"
        },
        {
          "field": "alternatePhoneNumber",
          "translationKey": "REGISTRATION_ALTERNATE_PHONE_NUMBER"
        }
      ]
    },
    "showExtraPatientIdentifiersSection": true
  },
  "fieldValidation": {
    "firstName": {
      "pattern": "^[a-zA-Z]+$",
      "errorMessage": "REGISTRATION_FIRST_NAME_VALIDATION"
    },
    "middleName": {
      "pattern": "^[a-zA-Z]+$",
      "errorMessage": "REGISTRATION_MIDDLE_NAME_VALIDATION"
    },
    "lastName": {
      "pattern": "^[a-zA-Z]+$",
      "errorMessage": "REGISTRATION_LAST_NAME_VALIDATION"
    },
    "email": {
      "pattern": "^(?=.{1,40}$)(([^<>()\\[\\]\\\\.,;:\\s@\"]+(\\.[^<>()\\[\\]\\\\.,;:\\s@\"]+)*)|(\".+\"))@(([a-zA-Z\\-0-9]+\\.)+[a-zA-Z]{2,4})$",
      "errorMessage": "REGISTRATION_EMAIL_VALIDATION"
    },
    "phoneNumber": {
      "pattern": "^\\+?[0-9]{6,15}$",
      "errorMessage": "REGISTRATION_PHONE_NUMBER_VALIDATION"
    },
    "alternatePhoneNumber": {
      "pattern": "^\\+?[0-9]{6,15}$",
      "errorMessage": "REGISTRATION_ALTERNATE_PHONE_NUMBER_VALIDATION"
    }
  },
  "extensionPoints": [
    {
      "id": "org.bahmni.registration.navigation",
      "description": "Navigation within registration first and second page"
    }
  ],
  "registrationAppExtensions": [
    {
      "id": "bahmni.registration.navigation.patient.start.visit",
      "extensionPointId": "org.bahmni.registration.navigation",
      "type": "startVisit",
      "order": 1,
      "requiredPrivilege": "View Patients"
    }
  ]
}